params {
    conffile            = ""
    fast5               = "$baseDir/test_dem/**/*.fast5"
    fastq               = ""

    reference           = "$baseDir/../anno/yeast_rRNA_ref.fa.gz"
    annotation          = ""
    ref_type            = "transcriptome"

    pars_tools          = "$baseDir/tool_opts/cdna_tool_readucks_opt.tsv"
    barcodes            = ""

    output              = "$baseDir/output"
    qualityqc           = 5
    granularity         = 1

    basecalling         = "guppy"
    GPU                 = "ON"
    demultiplexing      = "readucks"
    demulti_fast5       = "NO"

    filtering           = "nanofilt"

    mapping             = "minimap2"
    counting            = "nanocount"
    discovery           = "NO"

    cram_conv           = "YES"
    subsampling_cram    = 50

    saveSpace           = "NO"

    email               = "lucacozzuto@crg.es"
}
